package phaserHaxe.gameobjects.bitmaptext.typedefs;

/**
 *
 * @since 1.0.0
**/
typedef TintConfig =
{
	/**
	 * The top left tint value. Always zero in canvas.
	 *
	 * @since 1.0.0
	**/
	public var topLeft:Float;

	/**
	 * The top right tint value. Always zero in canvas.
	 *
	 * @since 1.0.0
	**/
	public var topRight:Float;

	/**
	 * The bottom left tint value. Always zero in canvas.
	 *
	 * @since 1.0.0
	**/
	public var bottomLeft:Float;

	/**
	 * The bottom right tint value. Always zero in canvas.
	 *
	 * @since 1.0.0
	**/
	public var bottomRight:Float;
};
