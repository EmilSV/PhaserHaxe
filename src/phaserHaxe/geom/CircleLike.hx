package phaserHaxe.geom;

typedef CircleLike =
{
	/**
	 * The x position of the center of the circle.
	 *
	 * @since 1.0.0
	**/
	var x:Float;

	/**
	 * The y position of the center of the circle.
	 *
	 * @since 1.0.0
	**/
	var y:Float;

	/**
	 * The internal radius of the circle.
	 *
	 * @since 1.0.0
	 */
	var radius:Float;
}
