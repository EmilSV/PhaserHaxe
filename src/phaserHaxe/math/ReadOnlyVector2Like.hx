package phaserHaxe.math;

/**
 * a object with a x and y component.
 *
 * @since 1.0.0
 */
typedef ReadOnlyVector2Like =
{
	/**
	 * The x component of this Vector.
	 *
	 * @since 1.0.0
	**/
	var x(default, never):Float;

	/**
	 * The y component of this Vector.
	 *
	 * @since 1.0.0
	**/
	var y(default, never):Float;
}
