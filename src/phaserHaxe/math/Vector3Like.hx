package phaserHaxe.math;

/**
 * a object with a x and y component.
 *
 * @since 1.0.0
**/
typedef Vector3Like =
{
	/**
	 * The x component of this Vector.
	 *
	 * @since 1.0.0
	**/
	var x:Float;

	/**
	 * The y component of this Vector.
	 *
	 * @since 1.0.0
	**/
	var y:Float;

	/**
	 * The z component of this Vector.
	 *
	 * @since 1.0.0
	**/
	var z:Float;
}
