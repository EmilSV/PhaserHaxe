package phaserHaxe.math;

/**
 * a object with a x and y component.
 *
 * @since 1.0.0
**/
typedef Vector4Like =
{
	/**
	 * The x component of this object.
	 *
	 * @since 1.0.0
	**/
	var x:Float;

	/**
	 * The y component of this object.
	 *
	 * @since 1.0.0
	**/
	var y:Float;

	/**
	 * The z component of this object.
	 *
	 * @since 1.0.0
	**/
	var z:Float;

	/**
	 * The w component of this object.
	 *
	 * @since 1.0.0
	**/
	var w:Float;
}
